[
    {
      "name": "Twitter API",
      "type": "API",
      "image": "public/images/twitter.png",
      "fields": [
        {
          "label": "Base URL",
          "name": "base_url",
          "type": "text",
          "required": true
        },
        {
          "label": "Authentication Type",
          "name": "auth_type",
          "type": "select",
          "options": ["No_Auth", "API_Key", "Bearer", "Basic_HTTP", "Session_Token", "OAuth"],
          "required": true
        },
        {
          "label": "API Key",
          "name": "auth_credentials[api_key]",
          "type": "text",
          "conditional": {
            "auth_type": "API_Key"
          }
        },
        {
          "label": "Inject Into",
          "name": "auth_credentials[inject_into]",
          "type": "select",
          "options": ["Query Parameter", "Header", "Body data (urlencoded form)", "Body JSON payload"],
          "conditional": {
            "auth_type": "API_Key"
          }
        },
        {
          "label": "Parameter Name",
          "name": "auth_credentials[parameter_name]",
          "type": "text",
          "conditional": {
            "auth_type": "API_Key"
          }
        },
        {
          "label": "Bearer Token",
          "name": "auth_credentials[token]",
          "type": "text",
          "conditional": {
            "auth_type": "Bearer"
          }
        },
        {
          "label": "Username",
          "name": "auth_credentials[username]",
          "type": "text",
          "conditional": {
            "auth_type": "Basic_HTTP"
          }
        },
        {
          "label": "Password",
          "name": "auth_credentials[password]",
          "type": "password",
          "conditional": {
            "auth_type": "Basic_HTTP"
          }
        },
        {
          "label": "Session Token",
          "name": "auth_credentials[session_token]",
          "type": "text",
          "conditional": {
            "auth_type": "Session_Token"
          }
        },
        {
          "label": "Token URL",
          "name": "auth_credentials[token_url]",
          "type": "text",
          "conditional": {
            "auth_type": "OAuth"
          }
        },
        {
          "label": "Client ID",
          "name": "auth_credentials[client_id]",
          "type": "text",
          "conditional": {
            "auth_type": "OAuth"
          }
        },
        {
          "label": "Client Secret",
          "name": "auth_credentials[client_secret]",
          "type": "password",
          "conditional": {
            "auth_type": "OAuth"
          }
        },
        {
          "label": "Scopes",
          "name": "auth_credentials[scopes]",
          "type": "text",
          "conditional": {
            "auth_type": "OAuth"
          }
        }
      ]
    },
